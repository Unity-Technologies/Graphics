project:
  name: URPUpdate_BoatAttack
  folder: BoatAttack-19-4
test_platforms:
  - type: playmode
  - type: editmode
platforms:
  - name: Win
    apis:
      - name: DX11
        cmd: -force-d3d11
      - name: Vulkan
        cmd: -force-vulkan
#  - name: OSX
#    apis:
#      - OpenGLCore
#      - Metal
build_configs:
- name: il2cpp_2
  scripting_backend: il2cpp
  api_level: NET_2_0
- name: il2cpp_4
  scripting_backend: il2cpp
  api_level: NET_4_6
- name: mono
  scripting_backend: mono
  api_level: NET_2_0
color_spaces:
  - Linear
  - gamma
all:
  dependencies:
    - platform: Win
      api: DX11
      build_config: il2cpp_4
      color_space: Linear
      test_platforms:
        - editmode
        - playmode
