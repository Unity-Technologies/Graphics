    surface.baseColor = OUTSG.${SHADERGRAPH_PARAM_BASECOLOR};
#endif

#if HAS_SHADERGRAPH_PARAM_NORMAL
    float3 n =  OUTSG.${SHADERGRAPH_PARAM_NORMAL};
    normalWS = mul(n,tbn);
#endif

surface.normalWS = normalWS;

