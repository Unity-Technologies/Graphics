.model-inspector {
    width: 260px;
    flex: 0 0 auto;
    height: 100%;
    border-left-color: #1a1a1a;
    border-left-width: 2px;
}

.model-inspector__title {
    padding: 5px;
    -unity-font-style: bold;
    border-bottom-color: #1a1a1a;
    border-bottom-width: 1px;
}

.model-inspector__container {
    margin: 5px;
}

.model-inspector__container .unity-inspector-element {
    padding: 0;
}
