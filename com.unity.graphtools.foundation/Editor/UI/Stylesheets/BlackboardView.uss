.blackboard-view {
    position: relative;
    flex-direction: column;
    width: auto;
    height: auto;
    background-color: var(--blackboard-background);
    font-size: var(--unity-font-size);
    flex: 1;
    overflow: hidden;
}

#overlay-popup .blackboard-view {
    width: 260px;
    height: auto;
}
